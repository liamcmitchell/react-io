{
  "name": "react-io",
  "version": "5.0.0",
  "description": "Data loading API",
  "files": [
    "dist"
  ],
  "main": "dist/react-io.cjs.js",
  "module": "dist/react-io.es.js",
  "es2015": "dist/react-io.es2015.js",
  "side-effects": false,
  "scripts": {
    "build": "rimraf dist && rollup -c",
    "dev": "rollup -c --watch",
    "test": "jest",
    "prepare": "npm run build",
    "precommit": "lint-staged",
    "prepush": "npm test"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add",
      "eslint"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "bracketSpacing": false,
    "arrowParens": "always"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./setupTest.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liamcmitchell/react-io.git"
  },
  "keywords": [
    "react"
  ],
  "author": "Liam Mitchell",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/liamcmitchell/react-io/issues"
  },
  "homepage": "https://github.com/liamcmitchell/react-io#readme",
  "dependencies": {
    "lodash": "^4.17.10",
    "prop-types": "^15.6.1",
    "react": "^16.0.0",
    "recompose": "^0.26.0",
    "rxjs": "^6.3.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^22.4.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.5.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-jest": "^21.15.1",
    "eslint-plugin-react": "^7.7.0",
    "husky": "^0.14.3",
    "jest": "^22.4.2",
    "lint-staged": "^7.2.2",
    "prettier": "1.11.1",
    "react-component-benchmark": "0.0.4",
    "react-dom": "^16.2.0",
    "rimraf": "^2.5.2",
    "rollup": "^0.56.5",
    "rollup-plugin-babel": "^3.0.4",
    "url-io": "^4.4.1"
  }
}
