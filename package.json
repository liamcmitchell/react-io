{
  "name": "react-io",
  "version": "5.1.0-beta.2",
  "description": "Data loading API",
  "files": [
    "dist"
  ],
  "main": "dist/react-io.cjs.js",
  "module": "dist/react-io.es.js",
  "es2015": "dist/react-io.es2015.js",
  "side-effects": false,
  "scripts": {
    "build": "rimraf dist && rollup -c",
    "dev": "rollup -c --watch",
    "test": "jest",
    "prepare": "npm run build"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "bracketSpacing": false,
    "arrowParens": "always"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/setupTest.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liamcmitchell/react-io.git"
  },
  "keywords": [
    "react"
  ],
  "author": "Liam Mitchell",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/liamcmitchell/react-io/issues"
  },
  "homepage": "https://github.com/liamcmitchell/react-io#readme",
  "dependencies": {
    "lodash": "^4.17.19",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "rxjs": "^6.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.0.0",
    "@rollup/plugin-babel": "^5.1.0",
    "@types/jest": "^26.0.7",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.1.0",
    "benchmark": "^2.1.4",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest": "^23.18.0",
    "eslint-plugin-react": "^7.20.4",
    "history": "^5.0.0",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "react-dom": "^16.13.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.22.2",
    "url-io": "^5.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  }
}
